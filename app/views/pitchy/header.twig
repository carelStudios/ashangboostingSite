.order-success-card {
   width:380px;
   background-color: #fff;
   border-radius:10px;
   position: fixed;
   top:0;
   left:50%;
   padding:0 30px 30px;
   color:#333;
   transform: translate(-50%,-50%) scale(0.1);
   visibility: hidden;
    transition :transform 0.4s,top 0.4s;
    box-shadow:0 20px 50px rgba(0,0,0,0.2);
    z-index:9999;
}
.order-success-card-active {
    visibility:visible;
    top:50%;
    transform: translate(-50%,-50%) scale(1);
}

.order-success-card img {
    width:100px;
    margin-top: -50px;
    border-radius:50%;
    box-shadow : 0 2px 5px rgba(0,0,0,0.2);
}
.order-success-card h2 {
    font-size: 23px;
    font-weight:500;
    margin : 30px 0 10px;
    text-align:center;
    
}
.order-success-card button {
    width:100%;
    margin-top:20px;
    padding:10px 0;
    background: #6fd649;
    outline:none;
    border:0;
    border-radius:6px;
    font-size:18px;
    cursor:pointer;
    box-shadow : 0 5px 5px rgba(0,0,0,0.2);
}
.order-success-card .order-details p {
    padding:10px;
}
.order-success-card .order-details .service-name {
 background-color:rgba(255, 190, 11,0.4);
 border:1px solid rgb(255, 190, 11);
 padding:8px;
 width:100%;
 border-radius:4px;
}
.order-success-card .order-details .paid-amount {
    float:left;
    background-color: rgba(120, 67, 219,0.2);
    padding : 4px;
    border-radius: 4px;
   border:1px solid rgb(120, 67, 219);
    margin-left:-10px;
}
.order-success-card .order-details .quantity {
    float:right;
    background-color:rgba(224, 20, 76,0.2);
    border:1px solid rgb(224, 20, 76);
    padding : 4px;
    border-radius: 4px;
    margin-right:-10px;
}
.order-success-card .user-details {
padding:8px;
background: rgba(111, 214, 73,0.2);
border:1px solid rgba(111, 214, 73);
margin-top:25px;
border-radius: 4px;
margin-bottom: -5px;
}
.dropdown-menu.open {
    max-width: 100%;
    position: inherit;
}
.open>.dropdown-menu {
    display: block;
}
.dropdown-menu {
    position: absolute;
    top: 100%;
    left: 0;
    z-index: 1000;
    display: none;
    min-width: 160px;
    padding: 5px 0;
    margin: 2px 0 0;
    font-size: 14px;
    text-align: left;
    background-color: #fff;
    border: 1px solid #ccc;
    border: 1px solid rgba(0, 0, 0, .15);
    border-radius: 4px;
    box-shadow: 0 6px 12px rgba(0, 0, 0, .175);
    background-clip: padding-box;
}
.img-responsive {
    max-width: calc(1em + 15px);
    max-height: calc(1em + 15px);
}
.bootstrap-select .dropdown-menu li a span.text {
    word-wrap: break-word;
    white-space: break-spaces;
}
.btn-default .badge {
    color: #f5f5f5;
    background-color: #111;
}

.table>tbody>tr>td, .table>tbody>tr>th, .table>tfoot>tr>td, .table>tfoot>tr>th, .table>thead>tr>td, .table>thead>tr>th {
